<div style="background-color: #877EB4" class="uk-width-2-3@m  uk-card uk-card-default uk-padding-small uk-visible@m">
  <div style="color: black" class="uk-grid">
    <div class="uk-width-1-6@m">
      <img src="/images/security-icon-14.jpg" width="60" alt="img" />
    </div>
    <div class="uk-width-expand@m uk-text-left">
      <div class="title">
        {{title}}
      </div>
      <div class="uk-text-muted uk-text-small">
        <div style="color: black">
          {{subTitle}}
        </div>
      </div>
    </div>
    <div class="uk-card uk-card-small uk-text-center uk-padding-remove">
      <a target="_blank" href="https://github.com/cathalohinse/Secure-Web-App">
        <div>
          <i class="fas fa-donate fa-3x" style="color:rgb(95, 96, 173)" title="Source repo" pos="bottom" uk-tooltip></i>
        </div>
        <div class="uk-width-expand@m uk-text-left">
          <div class="uk-text-muted uk-text-center" style="font-size:xx-small">Cathal O'hInse</div>
        </div>
      </a>
    </div>
  </div>
</div>
